{
  "manifest_version": 3,
  "name": "Mindful",
  "description": "A calm, visual space for your digital mind.",
  "version": "1.2.2",
  "key": "",

  "homepage_url": "https://www.mindfulbookmarks.com",
  "offline_enabled": true,

  "chrome_url_overrides": {
    "newtab": "NewTab.html"
  },

  "options_ui": {
    "page": "ManageAccount.html",
    "open_in_tab": true
  },

  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },

  "permissions": [
    "storage", 
    "tabs",
    "bookmarks",
    "history"
  ],

  "optional_permissions": [
  ],

  "host_permissions": [],

  "optional_host_permissions": [
    "https://api.mindfulbookmarks.com/*",
    "https://app.posthog.com/*"
  ],

  "action": {
    "default_popup": "PopUp.html",
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "128": "assets/icon-128.png"
    }
  },

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://cognito-idp.us-west-1.amazonaws.com https://cognito-identity.us-west-1.amazonaws.com https://kT7PGxGVd.auth.us-west-1.amazoncognito.com https://api.mindfulbookmarks.com https://app.posthog.com https://eidotpc2fc.execute-api.us-west-1.amazonaws.com;"
  }
}
